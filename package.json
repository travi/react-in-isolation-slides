{
  "name": "presentations",
  "license": "MIT",
  "author": "Matt Travi",
  "repository": "travi/presentations",
  "bugs": "https://github.com/travi/presentations/issues",
  "homepage": "htpps://presentations.travi.org",
  "scripts": {
    "lint:js": "eslint . --cache",
    "lint:md": "globstar --node -- markdownlint **/*.md",
    "test:smoke": "mocha --timeout 5000 --recursive test",
    "test": "run-s lint:* test:* build",
    "build": "webpack --env production",
    "build:dev": "webpack --env development",
    "start": "babel-node webpack-dev-server.js",
    "deploy": "gh-pages -r \"https://$GH_TOKEN@github.com/travi/presentations.git\" -a -d lib/",
    "commitmsg": "validate-commit-msg",
    "precommit": "npm test"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@travi/any": "1.4.3",
    "@travi/eslint-config-travi": "1.5.6",
    "assets-webpack-plugin": "3.5.1",
    "babel-loader": "7.1.2",
    "babel-plugin-lodash": "3.2.11",
    "babel-plugin-transform-react-jsx-source": "6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.9",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2015-node": "6.1.1",
    "babel-preset-react": "6.24.1",
    "babel-register": "6.26.0",
    "chai": "4.1.2",
    "clean-webpack-plugin": "0.1.17",
    "component-emitter": "1.2.1",
    "component-type": "1.2.1",
    "css-loader": "0.28.7",
    "cz-conventional-changelog": "2.0.0",
    "ease-component": "1.0.0",
    "enzyme": "2.9.1",
    "file-loader": "0.11.2",
    "gh-pages": "1.0.0",
    "globstar": "1.0.0",
    "html-webpack-plugin": "2.30.1",
    "husky": "0.14.3",
    "img-loader": "2.0.0",
    "jsdom": "11.2.0",
    "lodash-webpack-plugin": "0.11.4",
    "markdownlint-cli": "0.3.1",
    "mocha": "3.5.3",
    "mq-polyfill": "1.1.8",
    "mustache-loader": "1.2.0",
    "npm-run-all": "4.1.1",
    "quibble": "0.5.1",
    "raw-loader": "0.5.1",
    "style-loader": "0.19.0",
    "url-loader": "0.6.2",
    "validate-commit-msg": "2.14.0",
    "webpack": "3.6.0",
    "webpack-config-utils": "2.3.0",
    "webpack-dev-server": "2.8.1",
    "webpack-hot-middleware": "2.19.1",
    "webpack-visualizer-plugin": "0.1.11"
  },
  "dependencies": {
    "normalize.css": "7.0.0",
    "react": "15.6.2",
    "react-dom": "15.6.2",
    "react-emojione": "4.0.2",
    "react-typist": "1.1.1",
    "spectacle": "2.2.2",
    "spectacle-code-slide": "0.4.0",
    "spectacle-image-slide": "1.0.1",
    "spectacle-terminal": "0.3.0"
  }
}
